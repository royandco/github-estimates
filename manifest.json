{
	"name": "GitHub Estimates",
	"version": "0.1.5",
	"manifest_version": 2,
	"description": "Provide estimates on GitHub issues.",
	"author": "Roy & Co",
	"content_scripts": [
		{
			"matches": [
				"https://github.com/*",
				"http://github.com/*"
			],
			"js": [
				"bower_components/jquery/dist/jquery.min.js",
				"main.js"
			],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": [
		"menu.html",
		"event.html",
		"overview.html"
	],
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}